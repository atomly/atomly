generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

model posts {
  body      String
  comments  Int
  id        Int      @id
  likes     Int
  timestamp DateTime
  userId    users?
}

model users {
  email    String  @unique
  id       String  @id
  password String
  postses  posts[]
}