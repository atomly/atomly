generator client {
  provider = "prisma-client-js"
}

// The env("NAME") parameters come from a .env file.
// README.md for more info.
datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

model users {
  age       Int
  email     String @unique
  firstName String
  gametag   String
  id        Int    @id
  lastName  String
  password  String
}
