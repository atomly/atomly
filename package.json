{
  "name": "beast-backend",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "scripts": {
    "build": "npm -s run generate && tsc",
    "generate:prisma": "npx prisma2 generate",
    "generate:schema": "ts-node ./src/schema/generateSchema.ts",
    "generate:types": "npx gql2ts ./src/schema/schema.graphql -o ./src/types/schema.d.ts",
    "introspect:prisma": "npx prisma2 introspect",
    "start": "ts-node-dev --respawn ./src/index.ts",
    "start:prod": "ts-node --files ./src/prod.ts",
    "synchronize:typeorm": "ts-node --files ./src/synchronize_typeorm.ts",
    "test": "npx jest --watch"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview021",
    "bcrypt": "^3.0.8",
    "class-validator": "^0.11.0",
    "connect-redis": "^4.0.4",
    "dotenv": "^8.2.0",
    "express-session": "^1.17.0",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.6",
    "graphql-yoga": "^1.18.3",
    "ioredis": "^4.14.1",
    "merge-graphql-schemas": "^1.7.6",
    "pg": "^7.17.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "8.6.2",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-redis": "0.0.13",
    "@types/faker": "^4.1.9",
    "@types/ioredis": "^4.14.7",
    "@types/jest": "^25.1.1",
    "@types/jest-dev-server": "^4.2.0",
    "@types/node": "^13.5.0",
    "@types/shelljs": "^0.8.6",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "graphql-tag": "^2.10.3",
    "jest": "^25.1.0",
    "module-alias": "^2.2.2",
    "prettier": "^1.19.1",
    "prisma2": "^2.0.0-preview021",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "_moduleAliases": {
    "@root": "src/",
    "@entity": "src/entity",
    "@typings": "src/types",
    "@generatedPrisma": "./prisma",
    "@resolvers": "src/resolvers",
    "@utils": "src/utils",
    "@tests": "src/tests"
  }
}
