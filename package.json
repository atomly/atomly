{
  "name": "beast-backend",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "node": ">=8.10.0"
  },
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/faker": "^4.1.9",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.5.0",
    "@types/shelljs": "^0.8.6",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "graphql-tag": "^2.10.3",
    "jest": "^25.1.0",
    "jest-dev-server": "^4.4.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "prisma2": "^2.0.0-preview021",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview021",
    "@types/bcrypt": "^3.0.0",
    "@types/jest-dev-server": "^4.2.0",
    "bcrypt": "^3.0.8",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.6",
    "graphql-yoga": "^1.18.3",
    "merge-graphql-schemas": "^1.7.6",
    "module-alias": "^2.2.2",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "8.6.2",
    "typeorm": "^0.2.22",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "build": "npm -s run generate && tsc",
    "start": "nodemon --exec ts-node --files ./src/index.ts",
    "start:prod": "nodemon --exec ts-node --files ./src/prod.ts",
    "start:test": "nodemon --exec ts-node --files ./src/jest.ts",
    "test": "npm run test:db_setup && npx jest --watch",
    "test:db_setup": "ts-node --files ./src/tests/utils/setup.ts",
    "synchronize:typeorm": "ts-node --files ./src/synchronize_typeorm.ts",
    "introspect:prisma": "npx prisma2 introspect",
    "generate:prisma": "npx prisma2 generate",
    "generate:schema": "ts-node ./src/schema/generateSchema.ts",
    "generate:types": "npx gql2ts ./src/schema/schema.graphql -o ./src/types/schema.d.ts"
  },
  "_moduleAliases": {
    "@root": "src/",
    "@entity": "src/entity",
    "@typings": "src/types",
    "@generatedPrisma": "./prisma",
    "@resolvers": "src/resolvers",
    "@utils": "src/utils",
    "@tests": "src/tests"
  }
}
